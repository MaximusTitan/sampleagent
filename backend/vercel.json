{
    "version": 2,
    "builds": [
      {
        "src": "backend/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/main.py"
      }
    ],
    "functions": {
      "backend/*": {
        "includeFiles": ["requirements.txt", "backend/**"]
      }
    }
  }
  